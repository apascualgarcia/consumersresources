#!/bin/bash

# get plot for given matrix
# nohup ./build/compute_average_extinctions ./config/configuration_comparison_NR25_NS25.in path_to_save_file=./data_output/average_extinctions_large.out path_to_food_matrix=./matrices/Nr25_Nc25/RandTrix_Nr25_Nc25_Nest0.3_Conn0.1296.txt > ./logs/av_ext_large.log &
# nohup ./build/compute_average_extinctions_window ./config/configuration_comparison_NR25_NS25.in path_to_save_file=./data_output/average_extinctions_window.out path_to_food_matrix=./matrices/Nr25_Nc25/RandTrix_Nr25_Nc25_Nest0.3_Conn0.1296.txt > ./logs/av_ext_window.log &
# get delta critical (split in 6)
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_1.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_1.out seed_number=50 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_1.log &
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_2.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_2.out seed_number=60 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_2.log &
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_3.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_3.out seed_number=70 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_3.log &
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_4.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_4.out seed_number=80 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_4.log &
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_5.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_5.out seed_number=90 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_5.log &
nohup ./build/compute_critical_Delta_matrices ./config/configuration_comparison_NR25_NS25.in  path_to_food_matrix=./config/matrix_list_NR25_NS25_6.in path_to_save_file=./data_output/critical_delta_NR25_NS25_otherseed_6.out seed_number=100 |ts '[%Y-%m-%d %H:%M:%S]'> ./logs/critical_delta_os_6.log &
